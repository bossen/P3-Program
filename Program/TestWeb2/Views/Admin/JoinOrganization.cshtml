@model Model.Organization

@{
    ViewBag.Title = "JoinOrganization";
}



<table>
    <tr>
        <td>
            <h2>Almost done!</h2>

            You're about to join @Html.DisplayFor(model => model.Name)

            You have to following two options below.
        </td>
    </tr>
    <tr>
        <td>
            <button onclick="location.href='@Url.Action("Index", "Organization")';return false;">Join</button>
        </td>
        <td>
            <button onclick="location.href='@Url.Action("Organizations", "Admin")';return false;">Cancel</button>
        </td>
    </tr>
</table>


