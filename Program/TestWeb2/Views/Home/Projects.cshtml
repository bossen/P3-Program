<hgroup class="title">
    <h1>@ViewBag.Title.</h1>
    <h2>@ViewBag.Message</h2>
</hgroup>
<ul>
    @foreach (Model.VolunteerProject project in ViewBag.Projects)
    {
        <li>
            @project.Title
            @project.Time
            @*@project.Owner.Name*@
            @Html.ActionLink("details", "project", new { id = project.Id })
        </li>
    }
</ul>